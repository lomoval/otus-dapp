# Ethereum Environment Variables Example
# Copy this file to .env and fill in your actual values

# Sepolia Testnet RPC URL (from Infura, Alchemy, or other provider)
SEPOLIA_URL=https://sepolia.infura.io/v3/YOUR-PROJECT-ID

# Private key of the wallet used for deployment (without 0x prefix)
PRIVATE_KEY=your_wallet_private_key_here

# Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Optional: Hoodi Testnet (alternative to Sepolia)
HOODI_URL=https://hoodi.infura.io/v3/YOUR-PROJECT-ID

# Optional: Mainnet (for production deployment)
MAINNET_URL=https://mainnet.infura.io/v3/YOUR-PROJECT-ID

# Optional: Gas reporter configuration
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here
REPORT_GAS=true

# Optional: Coverage reporting
COVERAGE=true
